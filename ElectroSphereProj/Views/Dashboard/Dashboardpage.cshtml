@model List<ElectroSphereProj.ViewModel.ProductListViewModel>

@{
     Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="http://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>

<div id="2" class="mb-2">
    <section>
            <div id="containerSlider">
                <div id="slidingImage" class="rounded"> <img src="~/images/img1.jpg" alt="image1" class="sliding-img rounded shadow " > </div>
                <div id="slidingImage" class="rounded"> <img src="~/images/img2.jpg" alt="image2" class="sliding-img rounded shadow " > </div>
                <div id="slidingImage" class="rounded"> <img src="~/images/img3.jpg" alt="image3" class="sliding-img rounded shadow " > </div>
                
            </div>
    </section>
</div>
<div class="product-partial" id = "product-partial">
    @Html.Partial("_Products.cshtml",Model)
</div>
<script>
        $(document).ready(function()
        {
        $('#containerSlider').slick({
            dots: true,
            infinite: true,
            slidesToShow: 1,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 2000,
            });
        });
</script>


